# MaxLab 개발 환경 설정
# 개발 환경에서 사용할 기본 설정

# 환경 설정
ENVIRONMENT=development
DEBUG=true

# 데이터베이스 설정
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/max_lab

# SSL 설정 (개발 환경에서는 비활성화)
DB_SSL_MODE=disable

# 보안 키 (개발용)
SECRET_KEY=max-lab-dev-secret-key
JWT_SECRET_KEY=max-lab-dev-jwt-secret-key

# 외부 인증 서버
AUTH_SERVER_URL=http://localhost:8000
AUTH_SERVER_TIMEOUT=10

# 서버 설정
HOST=0.0.0.0
PORT=8010

# CORS 설정 (개발 환경)
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:3007","http://localhost:3008","http://localhost:3010","http://localhost:5173","http://localhost:8000","http://localhost:8001"]

# 로깅 레벨
LOG_LEVEL=INFO